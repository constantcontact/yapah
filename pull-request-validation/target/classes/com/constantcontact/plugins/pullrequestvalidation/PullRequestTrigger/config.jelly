<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:f="/lib/form">
  <f:entry title="Cron Schedule" field="spec" help="/descriptor/hudson.triggers.TimerTrigger/help/spec">
    <f:textarea checkUrl="'${rootURL}/trigger/TimerTrigger/check?value='+encodeURIComponent(this.value)"/>
  </f:entry>
  <f:entry title="Github Repository Configurtation" field="config">
    <f:repeatable var="it" name="config" items="${instance.config}" minimum="1">
      <table width="100%">
        <j:invokeStatic var="descriptor"
      className="com.constantcontact.plugins.pullrequestvalidation.PullRequestTriggerConfig"
      method="getClassDescriptor"/>
  <f:entry title="${%Github Repository}" field="gitHubRepository">
    <f:textbox value="${it.gitHubRepository}"/>
  </f:entry>
  <f:entry title="${%Repository Owner}" field="repositoryOwner">
    <f:textbox value="${it.repositoryOwner}"/>
  </f:entry>
  <f:entry title="${%Repository Name}" field="repositoryName">
    <f:textbox value="${it.repositoryName}"/>
  </f:entry>
  <f:entry title="${%Systen User}" field="systemUser">
    <f:textbox value="${it.systemUser}"/>
  </f:entry>
   <f:entry title="${%Systen User Password}" field="systemUserPassword">
    <f:password value="${it.systemUserPassword}"/>
  </f:entry>
      </table>
       <f:repeatableDeleteButton />
    </f:repeatable>
  </f:entry>
</j:jelly>